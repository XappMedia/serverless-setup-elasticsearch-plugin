    "chai": "4.2.0",
    "mocha": "5.2.0",
    "nyc": "13.1.0",
    "sinon": "7.2.2",
    "sinon-chai": "3.3.0",
    "standard-version": "4.4.0",
    "ts-node": "7.0.1",
    "tslint": "5.12.0",
